<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size android:width="84dp" android:height="84dp" />
            <solid android:color="#FFFFF6FD" />
            <stroke android:width="2dp" android:color="#FFFC9FD2" />
        </shape>
    </item>

    <item
        android:gravity="start|center_vertical"
        android:width="10dp"
        android:height="13dp"
        android:left="33dp">
        <shape android:shape="oval">
            <solid android:color="#FF2B2F3B" />
        </shape>
    </item>

    <item
        android:gravity="end|center_vertical"
        android:width="10dp"
        android:height="13dp"
        android:right="33dp">
        <shape android:shape="oval">
            <solid android:color="#FF2B2F3B" />
        </shape>
    </item>

    <item
        android:gravity="center_horizontal|center_vertical"
        android:width="14dp"
        android:height="8dp"
        android:top="18dp">
        <shape android:shape="oval">
            <solid android:color="#FF2B2F3B" />
        </shape>
    </item>

    <item
        android:gravity="start|center_vertical"
        android:width="18dp"
        android:height="9dp"
        android:left="24dp"
        android:top="10dp">
        <shape android:shape="oval">
            <solid android:color="#80FF9FCF" />
        </shape>
    </item>

    <item
        android:gravity="end|center_vertical"
        android:width="18dp"
        android:height="9dp"
        android:right="24dp"
        android:top="10dp">
        <shape android:shape="oval">
            <solid android:color="#80FF9FCF" />
        </shape>
    </item>

    <item
        android:gravity="top|end"
        android:width="14dp"
        android:height="14dp"
        android:right="20dp"
        android:top="18dp">
        <shape android:shape="oval">
            <solid android:color="#FFFFFFFF" />
        </shape>
    </item>
</layer-list>
